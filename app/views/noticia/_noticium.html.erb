<div id="<%= dom_id noticium %>">

  <div class="container">
    <div class="row">

      <div class="col-1">
        <p>
          <strong>Id:</strong>
            <%= noticium.id %>
        </p>
      </div>


      <div class="col-3">
        <p>
          <strong>Encabezado:</strong>
            <%= noticium.encabezado %>
        </p>
      </div>


      <div class="col-4">
        <p>
          <strong>Cuerpo:</strong>
            <%= noticium.cuerpo %>
        </p>
      </div>


      <div class="col-3">
        <p>
          <strong>Link:</strong>
            <%= noticium.link %>
      </p>


      </div>
        <div class="col-1">
          <p>
            <strong>Tipo:</strong>
              <%= noticium.tipo %>
        </p>
      </div>

    </div>
  </div>


